{
    "AWSEBDockerrunVersion": "2",
    "volumes": [
      {
        "name": "home-v",
        "host": {
          "sourcePath": "/var/app/current/app"
        }
      }
    ],
    "containerDefinitions": [
      {
        "name": "home",
        "image": "470118992782.dkr.ecr.ap-northeast-1.amazonaws.com/streamini:latest",
        "essential": true,
        "memory": 2048,
        "mountPoints": [
          {
            "sourceVolume": "home-v",
            "containerPath": "/works/streamini",
            "readOnly": false
          }
        ],
        "portMappings": [
          {
            "hostPort": 8443,
            "containerPort": 8443
          },
          {
            "hostPort": 8080,
            "containerPort": 8080
          }
        ],
        "command": ["dart", "./bin/main.dart","-p8080"]
      }
    ]
}  

  